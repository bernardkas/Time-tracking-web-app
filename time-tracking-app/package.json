{
  "name": "icuem-time-tracking",
  "version": "0.0.1",
  "description": "This is a time tracking app",
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf dist app/build",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "electron --inspect=9229 .",
    "dev:watch": "npx electronmon --trace-warnings .",
    "build": "cd app && npm run build",
    "package": "electron-builder",
    "package:mac": "electron-builder --mac",
    "package:win": "electron-builder --win",
    "package:linux": "electron-builder --linux"
  },
  "build": {
    "extends": null,
    "appId": "com.icuem.timetracking",
    "productName": "icuem time tracking",
    "directories": {
      "output": "dist"
    },
    "extraResources": [],
    "files": [
      "main.js",
      "preload.js",
      "package.json",
      "controllers/**/*",
      "middleware/**/*",
      "routes/**/*",
      "build/**/*",
      "!**/node_modules/**/*",
      "!**/.gitignore",
      "!**/.editorconfig",
      "!**/.prettierrc"
    ],
    "asar": true,
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "keywords": [
    "time",
    "tracking"
  ],
  "author": "Bernard",
  "license": "ISC",
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@testing-library/react": "^12.1.5",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "electron": "^34.0.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-global-key-listener": "^0.3.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.15",
    "electron-builder": "^25.1.8"
  }
}
